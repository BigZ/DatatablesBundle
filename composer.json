{
    "name": "bigz/datatablesbundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Datatable Bundle For Doctrine2 Entities",
    "keywords": ["symfony2", "datatable", "grid", "table", "datagrid", "pagination"],
    "homepage": "https://github.com/stwe/DatatablesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "BigZ"
        },
        {
            "name": "SgDatatablesBundle contributors",
            "homepage": "https://github.com/stwe/DatatablesBundle/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.6",
        "friendsofsymfony/jsrouting-bundle": "@stable"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3"
    },
    "suggest": {
        "components/jquery": "1.11.3",
        "datatables/datatables": "1.10.10",
        "moment/moment": "2.10.6",
        "liip/imagine-bundle": "^1.3"
    },
    "autoload": {
        "psr-0": { "Sg\\DatatablesBundle": "" }
    },
    "target-dir": "BigZ/DatatablesBundle"
}
